{% extends 'base.html.twig' %}

{% block title %}Contato{% endblock %}

{% block body %}
    <h1>Contato</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contato.id }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ contato.nome }}</td>
            </tr>
            <tr>
                <th>Endereco</th>
                <td>{{ contato.endereco }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ contato.email }}</td>
            </tr>
            <tr>
                <th>Telefone</th>
                <td>{{ contato.telefone }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ contato.created ? contato.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ contato.isactive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('contato_index') }}">back to list</a>

    <a href="{{ path('contato_edit', {'id': contato.id}) }}">edit</a>

    {{ include('contato/_delete_form.html.twig') }}
{% endblock %}
