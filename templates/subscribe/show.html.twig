{% extends 'base.html.twig' %}

{% block title %}Subscribe{% endblock %}

{% block body %}
    <h1>Subscribe</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subscribe.id }}</td>
            </tr>
            <tr>
                <th>FullName</th>
                <td>{{ subscribe.fullName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ subscribe.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ subscribe.telephone }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ subscribe.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ subscribe.created ? subscribe.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('subscribe_index') }}">back to list</a>

    <a href="{{ path('subscribe_edit', {'id': subscribe.id}) }}">edit</a>

    {{ include('subscribe/_delete_form.html.twig') }}
{% endblock %}
